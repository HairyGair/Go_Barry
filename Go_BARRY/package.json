{
  "name": "go-barry-app",
  "version": "3.0.0",
  "description": "BARRY - Traffic Intelligence for Go North East",
  "main": "node_modules/expo-router/entry",
  "scripts": {
    "start": "expo start --web",
    "dev": "expo start --web --dev-client",
    "web": "expo start --web",
    "mobile": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "npm run build:web",
    "build:web": "expo export --platform web --output-dir dist",
    "build:web:production": "NODE_ENV=production expo export --platform web --output-dir dist",
    "build:cpanel": "NODE_ENV=production expo export --platform web --output-dir cpanel-build --clear",
    "build:cpanel:zip": "npm run build:cpanel && cd cpanel-build && zip -r ../gobarry-cpanel-deployment.zip . -x '*.DS_Store' '*.map'",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "build:preview": "eas build --platform all --profile preview",
    "serve": "npx serve dist -p 3000",
    "serve:cpanel": "npx serve cpanel-build -p 3000",
    "deploy:preview": "npm run build:web && npm run serve",
    "deploy:cpanel": "npm run build:cpanel:zip && echo 'Upload gobarry-cpanel-deployment.zip to your cPanel File Manager'",
    "publish": "expo publish",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update": "eas update",
    "reset": "expo start --clear"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-picker/picker": "^2.11.0",
    "@supabase/supabase-js": "^2.49.8",
    "expo": "53.0.10",
    "expo-font": "~13.3.1",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.8",
    "mapbox-gl": "^3.12.0",
    "maplibre-gl": "^5.6.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react-native": "^0.72.8",
    "serve": "^14.2.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "react-native",
    "expo",
    "traffic",
    "alerts",
    "go-north-east"
  ],
  "author": "Anthony Gair",
  "license": "MIT",
  "private": true
}
