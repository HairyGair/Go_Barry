# BARRY Backend Environment Variables
# Copy this file to .env and add your real API keys

# Server Configuration
PORT=3001
NODE_ENV=development

# API Keys - Replace with your actual keys
NATIONAL_HIGHWAYS_API_KEY=your_national_highways_key_here
TOMTOM_API_KEY=your_tomtom_key_here
MAPBOX_TOKEN=your_mapbox_token_here
HERE_API_KEY=your_here_key_here
MAPQUEST_API_KEY=your_mapquest_key_here

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# CORS Settings
CORS_ORIGIN=http://localhost:8081
RENDER_BACKEND_URL=https://go-barry.onrender.com

# Logging
LOG_LEVEL=info